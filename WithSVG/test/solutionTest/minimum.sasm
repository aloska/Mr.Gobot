/*
Проверка работоспособности решателя - программа на асме вычисляет минимум из 10 значений
В In[0][0-9] лежат числа, от которых надо посчитать минимум
В Out[0][0] лежит результат 
*/
asm:	
	li 		x31, 8
	li		x30, 0			
	li 		x29, 0
	ldinx	x0, x29, x30	lbne:	//загрузить в x0 число из входа 
	push	x0						//в стек
	addi 	x30, x30, 1
	bne		x30, x31, -3 	:lbne				
	//упорядочиваем по возрастанию	
	//найдем сначала минимальное
	li		x30, 0			//ну как бы 0 для сравнения
	pop		x1				//label1:
	ble		x0, x1, 2
	addi	x0, x1, 0		//по сути x[0]=x[1]
	subi	x31, x31, 1
	beq		x31, x30, 2
	jmp		-5				//:label1
	//теперь в x0 минимальный, запишем в память (а шо? надо проверить)
	stout	0, 0, x0 
	